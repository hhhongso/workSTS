package sample05;

public class ScoreDisp implements Score{
	private ScoreDTO scoreDTO; 
	
	@Override
	public void execute() {
		scoreDTO = getScoreDTO();
		scoreDTO.setTot(scoreDTO.getKor() + scoreDTO.getEng() + scoreDTO.getMath());
		scoreDTO.setAvg(scoreDTO.getTot() / 3.0);
		
		System.out.println(getScoreDTO());
		
	}

}
