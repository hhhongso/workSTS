package sample05;


import java.util.ArrayList;

import javax.annotation.Resource;


import lombok.Data;

@Data
public class ScoreDisp implements Score{
	 @Resource private ArrayList<ScoreDTO> list;
	
	@Override
	public void execute() {
		System.out.println("input에서 list의 주소값: "+ getList());
		System.out.println("disp에서 list 개수: " + getList().size());
		for(ScoreDTO scoreDTO : getList()) {
			System.out.println(scoreDTO.toString());			
		}
		
		
	}

}
