package sample05;


import java.util.ArrayList;

import javax.annotation.Resource;

import org.springframework.beans.factory.annotation.Autowired;

import lombok.Data;

@Data
public class ScoreDisp implements Score{
	@Autowired @Resource private ArrayList<ScoreDTO> list;
	
	@Override
	public void execute() {
		System.out.println("disp에서 list의 주소값: "+ list);
		System.out.println("disp에서 list 개수: " + list.size());
		for(ScoreDTO scoreDTO : list) {
			System.out.println(scoreDTO.toString());			
		}
		
		
	}

}
