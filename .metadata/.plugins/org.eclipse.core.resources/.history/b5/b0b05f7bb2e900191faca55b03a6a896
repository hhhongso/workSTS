package sample05;

import java.lang.annotation.Inherited;
import java.util.ArrayList;

import org.springframework.beans.factory.annotation.Autowired;

import lombok.Data;

@Data
public class ScoreDisp implements Score{
	 private ArrayList<ScoreDTO> list;
	
	@Override
	public void execute() {
		System.out.println("input에서 list의 주소값: "+ getList());
		System.out.println("disp에서 list 개수: " + list.size());
		for(ScoreDTO scoreDTO : list) {
			System.out.println(scoreDTO.toString());			
		}
		
		
	}

}
